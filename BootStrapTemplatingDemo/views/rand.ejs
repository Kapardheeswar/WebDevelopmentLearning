<%- include("partials/head.ejs") %>
<body>
<%- include("partials/navbar.ejs") %>
<h1>Your Random Number is :<%= num %></h1>
<% if(num%2===0){ %>
    <h2>This is an Even Number</h2>
<% } %>
<% if(num%2!==0){ %>
    <h2>This is an Odd Number</h2>
<% } %>
<h3><%= num%2===0 ? "EVEN!!":"ODD!!" %></h3>
</body>
</html>